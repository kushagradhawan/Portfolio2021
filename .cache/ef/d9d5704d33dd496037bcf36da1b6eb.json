: \"nmachina-bold\";\n    src: url(\"./assets/fonts/nmachina-bold.otf\") format(\"opentype\");\n}\n@font-face {\n    font-family: \"nmontreal-regular\";\n    src: url(\"./assets/fonts/nmontreal-regular.otf\") format(\"opentype\");\n}\n@font-face {\n    font-family: \"nmontreal-medium\";\n    src: url(\"./assets/fonts/nmontreal-medium.otf\") format(\"opentype\");\n}\n@font-face {\n    font-family: \"nmontreal-bold\";\n    src: url(\"./assets/fonts/nmontreal-bold.otf\") format(\"opentype\");\n}\n\n.a-title {\n    text-transform: uppercase;\n    font-family: \"nmontreal-medium\";\n}\n.a-title--lg {\n    font-size: 12rem;\n    line-height: 10rem;\n    letter-spacing: -0.04em;\n}\n.a-title--md {\n    font-size: 7rem;\n    line-height: 96%;\n    letter-spacing: -0.04em;\n}\n.a-title--sm {\n    font-size: 2rem;\n    line-height: 124%;\n    letter-spacing: -0.04em;\n}\n\n.a-headline {\n    font-family: \"NeueMontreal-Medium\";\n    font-size: 1.5rem;\n    line-height: 116%;\n}\n\n.a-callout {\n    font-family: \"NeueMontreal-Medium\";\n    font-size: 0.75rem;\n    line-height: 0.875rem;\n}\n\n.a-subhead {\n    font-family: \"NeueMontreal-Medium\";\n    font-size: 0.625rem;\n    line-height: 0.75rem;\n}\n\n.a-caption {\n    font-family: \"NeueMachina-Medium\";\n    text-transform: uppercase;\n    font-size: 0.625rem;\n    line-height: 1rem;\n}\n.a-caption--sm {\n    font-size: 0.5rem;\n    line-height: 0.75rem;\n}\n\n/* variables */\n$main-container-width: 79rem;\n$main-container-col-count: 8;\n$home-intro-image-height: 48rem;\n\n/* base styles */\n.a-grid {\n    display: grid;\n    grid-template-columns: repeat($main-container-col-count, 1fr);\n    column-gap: 1rem;\n    width: $main-container-width;\n    margin-left: calc((100vw - #{$main-container-width}) / 2);\n}\n.a-grid--no-margin {\n    margin-left: unset;\n}\n#a-webgl-container {\n    width: 100vw;\n    height: 100vh;\n    z-index: -2;\n    background: white;\n    position: fixed;\n    top: 0;\n    left: 0;\n}\n.a-background-stripes {\n    position: fixed;\n    z-index: -1;\n    top: 0;\n    left: 4rem;\n    height: 100vh;\n    width: calc(100vw - 8rem);\n    opacity: 0.08;\n    z-index: -1;\n}\n.a-background-stripes--vertical {\n    background: repeating-linear-gradient(90deg, white 0px, white 7px, black 7px, black 8px);\n}\n\n.alignment-grid {\n    position: fixed;\n    width: $main-container-width;\n    left: calc((100vw - #{$main-container-width}) / 2);\n    top: 0;\n    height: 100vh;\n    @extend .a-grid;\n    @extend .a-grid--no-margin;\n\n    div {\n        background-color: red;\n        opacity: 0.1;\n    }\n}\n\n/* navigation */\n.o-nav {\n    .m-project-links {\n        width: 100vw;\n        background: transparent;\n        position: fixed;\n        top: 0;\n        padding: 2rem calc((100vw - #{$main-container-width}) / 2);\n        display: grid;\n        grid-template-columns: repeat($main-container-col-count, 1fr);\n        column-gap: 1rem;\n        z-index: 10;\n\n        /* elements */\n        .m-link-item {\n            display: block;\n        }\n    }\n}\n\n/* home styles */\n.o-home {\n    @extend .a-grid;\n\n    .m-home__title {\n        grid-column: 1 / span $main-container-col-count;\n        margin: 6rem 0;\n    }\n\n    .m-home__intro {\n        grid-column: 1 / span $main-container-col-count;\n        @extend .a-grid;\n        @extend .a-grid--no-margin;\n        align-items: center;\n\n        .a-intro__image {\n            height: $home-intro-image-height;\n            grid-column: 1 / span 5;\n            justify-self: stretch;\n            transform: translate3d(-4rem, 0, 0);\n        }\n\n        .m-intro__text {\n            grid-column: 7 / span 2;\n        }\n    }\n\n    .o-home__work {\n        grid-column: 1 / span $main-container-col-count;\n        @extend .a-grid;\n        @extend .a-grid--no-margin;\n\n        .m-work__item {\n            margin-bottom: 2rem;\n            .m-work__thumbnails {\n                display: grid;\n                grid-template-columns: repeat(2, 1fr);\n                grid-template-rows: repeat(3, 16rem);\n                grid-template-areas: \"t1 t2\" \"t1 t3\" \"d t3\";\n                column-gap: 1rem;\n                row-gap: 1rem;\n\n                img {\n                    width: 100%;\n                    max-height: 100%;\n                }\n\n                .a-thumbnail__image-1 {\n                    grid-area: t1;\n                }\n                .a-thumbnail__image-2 {\n                    grid-area: t2;\n                }\n                .a-thumbnail__image-3 {\n                    grid-area: t3;\n                }\n                .m-work__description {\n                    grid-area: d;\n                    align-self: start;\n                    display: grid;\n                    grid-template-columns: repeat(3, 1fr);\n\n                    * {\n                        grid-column: 1 / span 1;\n                    }\n                }\n            }\n        }\n\n        .m-work__item--left {\n            grid-column: 1 / span 6;\n        }\n        .m-work__item--right {\n            grid-column: 3 / span 6;\n        }\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"67d1c34d639e643b8dcdf6c8e7892521","cacheData":{"env":{}}}